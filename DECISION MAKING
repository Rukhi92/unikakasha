    #SECE DINING
    x=input()
y=int(input())
if(x=='front'and y==1) or (x=='rear'and y==2):
    print("left handed")
elif(x=='front'and y==2) or (x=='rear' and y==1):
    print("right handed")
else:
    print("invalid input")
    #INTERSECTION OF CIRCLE
    import math
x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
d=math.sqrt(((x1-x2)**2)+((y1-y2)**2))
if d==r1+r2:
    print("the circles are tangential")
elif d<r1+r2:
    print("the circles overlap")
else:
    print("the circles do not overlap")
 #MICROWAVE OVEN
    print("Enter the number of items")
A=int(input())
print("Enter the single item heating time")
B=float(input())
x=B+(50/100)*B
y=B+2*B
if A==2:
    print("the recommended heating time is %.2f"%x)
elif A==3:
    print("the recommended heating time is %.2f"%y)
else:
    print("number of items is more")
    

