#GREATEST OF TWO NUMBERS
a=int(input('enter number:'))
b=int(input('enter number:'))
if (a==b):
       print(f"{a} is equal to {b}")
elif (a>b):
        print(f"{a} is greater than {b}")
else:
    print(f"{a} is less than {b}")
#VOWEL OR CONSONANT
s=input()
if s in"aeiouAEIOU":
    print("vowel")
elif s in "bcdfghjklmnpqrstvwxyzBCDFGHJKLMNPQRSTVWXYZ":
    print("consonant")
else:
    print("not an alphabet")
    #GRADING SYSTEM
    marks=int(input())
if marks==100:
    print("s")
elif 100>marks>=90:
    print("A")
elif 90>marks>=80:
    print("B")
elif 80>marks>=70:
    print("C")
elif 70>marks>=60:
    print("D")
elif 60>marks>=50:
    print("E")
elif 0<marks<50:
    print("F")
else:
    print("Invalid Input")
 #PROFIT OR LOSS
 cp=float(input("Enter the price of a dozen mangoes:"))
spofonemango=float(input("Enter the price at which 1 mango is being sold:"))
sp=spofonemango*12
if cp==sp:
    print("No profit nor loss")
elif sp>cp:
    print("profit: Rs.%.2f" %(sp-cp))
elif sp<cp:
    print("loss: Rs.%.2f" %(cp-sp))
    #FEE COLLECTION
    st=input("Enter the student type \n")
if st=="MSDS":
    print("Enter tuition fee")
    tf=float(input(""))
    print("Enter bus fee")
    bf=float(input(""))
    paid=tf+bf
    print("The fees to be paid by the student is Rs.%.2f"%paid)
elif st=="MSH":
    print("Enter tuition fee")
    tf=float(input(""))
    print("Enter hostel fee")
    hf=float(input(""))
    paid=tf+hf
    print("The fees to be paid by the student is Rs.%.2f"%paid)
elif st=="MGSDS":
    print("Enter tuition fee")
    tf=float(input(""))
    print("Enter bus fee")
    bf=float(input(""))
    paid=((150/100)*tf)+bf
    print("The fees to be paid by the student is Rs.%.2f"%paid)
else:
    print("Enter tuition fee")
    tf=float(input())
    print("Enter hostel fee")
    hf=float(input())
    paid=((150/100)*tf)+hf
    print("The fees to be paid by the student is Rs.%.2f"%paid)
    #Y2K PROBLEM DETECTOR
    print("Enter year of Birth")
bty=int(input(""))
print("Enter current year")
cy=int(input(""))
if bty<cy:
    age=cy-bty
    print("your age is",age)
else:
    bty=100-bty
    age=bty+cy
    print("your age is",age)
